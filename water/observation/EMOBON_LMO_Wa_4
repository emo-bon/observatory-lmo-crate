@base <http://data.emobon.embrc.eu/observatory-lmo-crate/water/observation/EMOBON_LMO_Wa_4> . 
<#chlorophyll>
    a sosa:Observation, schema:Observation , csvw:Cell ;
    sosa:observedProperty [
        a sosa:ObservableProperty ; 
        dct:identifier <http://vocab.nerc.ac.uk/collection/S27/current/CS002896/> ;
        rdfs:label 'chlorophyll'^^xsd:string ; 
    ] ;

    sosa:hasResult [
        a sosa:Result , qudt:QuantityValue ;
        qudt:numericValue '1.11'^^xsd:double ;
        qudt:unit <http://vocab.nerc.ac.uk/collection/P06/current/UGPL/> ; 
    ] ;
    schema:measurementTechnique 'Protocol based on Jespersen, A. M., and K. Christoffersen.1987. Measurements of chlorophyll-a from phytoplankton using ethanol as extraction solvent.'^^xsd:string ;
    sosa:hasFeatureOfInterest </observatory-lmo-crate/water/sample/EMOBON_LMO_Wa_4> ; 
.
